{
  "name": "workspace",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run turbo:build",
    "changeset": "changeset",
    "changeset:tag": "pnpm run changeset tag",
    "changeset:version": "pnpm run changeset version",
    "ci:publish": "pnpm run build && pnpm publish -r && pnpm run changeset:tag",
    "ci:version": "pnpm run changeset:version",
    "commitlint": "commitlint",
    "commitlint:edit": "pnpm run commitlint --edit",
    "dev": "pnpm run turbo:dev",
    "eslint": "eslint",
    "eslint:all": "pnpm run eslint .",
    "husky": "husky",
    "husky:commit-msg": "pnpm run commitlint:edit",
    "husky:pre-commit": "pnpm run lint-staged",
    "lint": "pnpm run prettier:write:all && pnpm run eslint:all",
    "lint-staged": "lint-staged",
    "lint:check:all": "pnpm run prettier:check:all && pnpm run eslint:all",
    "prepare": "pnpm run husky",
    "prettier": "prettier",
    "prettier:check": "pnpm run prettier --check",
    "prettier:check:all": "pnpm run prettier:check .",
    "prettier:write": "pnpm run prettier --write",
    "prettier:write:all": "pnpm run prettier:write .",
    "test": "pnpm run turbo:test",
    "turbo": "turbo",
    "turbo:build": "pnpm run turbo run build",
    "turbo:dev": "pnpm run turbo run dev",
    "turbo:test": "pnpm run turbo run test",
    "turbo:typecheck": "pnpm run turbo run typecheck",
    "typecheck": "pnpm run turbo:typecheck"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@eslint/js": "^9.7.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "^2.5.1",
    "turbo": "^2.1.3",
    "typescript-eslint": "^7.17.0"
  },
  "packageManager": "pnpm@9.12.1"
}
